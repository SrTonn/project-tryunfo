{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": "trybe-frontend",
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "overrides": [
        {
          "files": ["*.jsx", "*.js"]
        }
      ],
    "plugins": [
        "react"
    ], 
    "rules": {
        "react/state-in-constructor": ["off"]
    }
}
